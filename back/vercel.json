{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    { "source": "/api/:path*", "destination": "/app.js" },
    { "source": "/uploads/:path*", "destination": "/uploads/:path*" },
    { "source": "/:path*", "destination": "/app.js" }
  ]
}
